<?xml version="1.0" encoding="utf-8"?>
<com.jcminarro.roundkornerlayout.RoundKornerRelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:layout_marginBottom="@dimen/person_info_list_item_margin_between_list_items"
    android:background="@color/colorWhite"
    android:orientation="horizontal"
    app:corner_radius="@dimen/item_corner_radius">

    <com.google.android.material.textfield.TextInputLayout
        android:id="@+id/kid_information_list_param"
        style="@style/Widget.MaterialComponents.TextInputLayout.FilledBox.Dense"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:textColorHint="@color/colorLightGrey"
        android:textStyle="normal"

        app:boxStrokeWidth="2dp"
        app:endIconContentDescription="Kakayato huinya"
        app:endIconDrawable="@drawable/ic_delete_grey"
        app:endIconMode="clear_text"
        app:hintTextAppearance="@style/UserSetting.UserParams.Caption"
        app:hintTextColor="@color/colorLightGrey">

        <com.google.android.material.textfield.TextInputEditText
            android:id="@+id/kid_information_list_value"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:background="@color/colorWhite"
            android:singleLine="true"
            android:textAppearance="@style/UserSetting.UserParams.Text"
            android:textColor="@color/colorDarkGrey"
            android:textStyle="bold" />

    </com.google.android.material.textfield.TextInputLayout>
</com.jcminarro.roundkornerlayout.RoundKornerRelativeLayout>

